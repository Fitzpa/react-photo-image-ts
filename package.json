{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "init-msw": "msw init public/",
    "test-storybook": "test-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-photo-gallery-ts",
  "author": "Louie Fitzpatrick",
  "module": "dist/react-photo-gallery-ts.esm.js",
  "size-limit": [
    {
      "path": "dist/react-photo-gallery-ts.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-photo-gallery-ts.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/test-runner": "^0.5.0",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.1",
    "husky": "^8.0.1",
    "msw": "^0.44.2",
    "msw-storybook-addon": "^1.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "goober": "2.1.10",
    "react-image": "4.0.3",
    "react-resize-detector": "^7.1.2",
    "resize-observer-polyfill": "^1.5.1"
  }
